	<queryString>
		<![CDATA[
SELECT
    eccs_cliente.imagen as logo,
    -- ============== DATOS GENERALES DEL COMPROBANTE ==============
    eccs_tipo_comprobante.serie || '' || LPAD(eccs_comprobante_venta.folio::varchar, 7, '0') AS folio,
     '$' || to_char(coalesce(sum(eccs_comprobante_venta_info.descuento), 0), 'FM999,999,999.00') as comprobanteDesc,
    eccs_comprobante_venta.uuid,
    to_char(eccs_comprobante_venta.fecha_timbrado, 'YYYY-MM-DD"T"HH24:MI:SS') as fechatimbrado,
    
    -- ============== CALCULOS MONETARIOS ==============
    '$' || to_char(coalesce(sum(eccs_comprobante_venta_info.valor_unitario), 0), 'FM999,999,999.00') as subTotal,
    '$' || to_char((coalesce(sum(eccs_comprobante_venta_info.valor_unitario), 0) +
                COALESCE("arieserp_contabilidad".fn_get_TotalImpuestosTrasladados(1), 0) -
                COALESCE("arieserp_contabilidad".fn_get_TotalImpuestosRetenidos(1), 0))::numeric(10,2), 'FM999,999,999.00') as total,
    eccs_moneda.prefijo as "Moneda",
    eccs_moneda.tc::varchar as "TipoCambio",
    '02 Si objeto de impuesto' as satObj,

    -- ============== CLASIFICACION FISCAL ==============
    sat_comprobantes.indice || ' ' || sat_comprobantes.descripcion as "TipoDeComprobante",
    sat_exportacion.codigo  || ' ' || sat_exportacion.descripcion as "Exportacion",
    sat_metodo_pago.codigo  || ' ' || sat_metodo_pago.descripcion as "MetodoPago",
    sat_forma_pago.codigo   || ' ' || sat_forma_pago.descripcion as formaPago,
    eccs_tipo_comprobante.cp as "LugarExpedicion",

    -- ============== BLOQUE EMISOR ==============
    eccs_tipo_comprobante.rfc as rfcEmisor,
    eccs_tipo_comprobante.numerocertificado,
    eccs_tipo_comprobante.razonsocial as nombreEmisor,
    emisorRegimen.codigo || ' ' || emisorRegimen.descripcion as regimenFiscalEmisor,
    '+52 4651068560' as telefonoSucursal,
    'CP: ' || eccs_tipo_comprobante.cp || ', ' || arieserp_sucursal_domicilio.calle || ' ' || arieserp_sucursal_domicilio.num_ext || ', MÉXICO' as emisordomicilio,

    -- ============== BLOQUE RECEPTOR ==============
    eccs_cliente.rfc as rfccliente,
    eccs_cliente.nombre as cliente,
    'CP: ' || eccs_cliente_domicilio.cp || ', ' || eccs_cliente_domicilio.calle || ', MÉXICO' as cpcliente,
    sat_regimenfiscalcfdi.codigo || ' ' || sat_regimenfiscalcfdi.descripcion as regimencliente,
    sat_usocfdi.codigo || ' ' || sat_usocfdi.descripcion as usocfdicliente,

    -- ============== CONCEPTOS ==============
    sat_claveunidad.c_claveunidad as satclaveunidad,
    sat_claveprodserv.codigo as satunidad,
    eccs_producto_servicio.descripcion as concepto,
    eccs_comprobante_venta_info.cantidad,
    '$' || to_char(eccs_comprobante_venta_info.valor_unitario, 'FM999,999,999.00') as valor_unitario,
    coalesce(eccs_comprobante_venta_info.descuento, '0.0') as conceptoDescuento,

    -- ============== IMPUESTOS TRASLADOS ==============
    --'$' || to_char(TRUNC((eccs_comprobante_venta_info.valor_unitario * (1 - eccs_comprobante_venta_info.descuento / 100))), 'FM999,999,999.00') as impuesto_base_traslado,
    ---sat_impuesto_traslado.c_impuesto as impuesto_traslado,
    --sat_tipo_factor_traslado.descipcion as tipo_factor_traslado,
    --sat_impuesto_traslado.valor_maximo::varchar as tasa_cuota_traslado,
    '$' || to_char(COALESCE(((COALESCE(eccs_comprobante_venta_info.valor_unitario, 0) * (1 - COALESCE(eccs_comprobante_venta_info.descuento, 0) / 100)) * COALESCE(sat_impuesto_traslado.valor, 0)), 0), '999,999,990.00') as importe_traslado,
    '$' || to_char(COALESCE("arieserp_contabilidad".fn_get_TotalImpuestosTrasladados(1), 0), '999,999,990.00')as total_importe_traslado,

   -- ============== IMPUESTOS RETENCIONES ==============
    --'$' || to_char((eccs_comprobante_venta_info.valor_unitario * (1 - eccs_comprobante_venta_info.descuento / 100)), 'FM999,999,999.00') as impuesto_base_retencion,
    --sat_impuesto_retencion.c_impuesto::varchar as impuesto_retencion,
    --sat_tipo_factor_retencion.descipcion as tipo_factor_retencion,
    --sat_impuesto_retencion.valor_maximo::varchar as tasa_cuota_retencion,
    '$' || to_char(COALESCE(((COALESCE(eccs_comprobante_venta_info.valor_unitario, 0) * (1 - COALESCE(eccs_comprobante_venta_info.descuento, 0) / 100)) * COALESCE(sat_impuesto_retencion.valor, 0)), 0), '999,999,990.00') as importe_retencion,
    '$' || to_char(COALESCE("arieserp_contabilidad".fn_get_TotalImpuestosRetenidos(1), 0), '999,999,990.00')as total_importe_retencion,

    -- ============== FOOTER ==============
    -- Total con Letra usando función TOTAL_LETRA:
    "eccs".TOTAL_LETRA((eccs_comprobante_venta_info.valor_unitario +
                        COALESCE("arieserp_contabilidad".fn_get_TotalImpuestosTrasladados(1), 0) -
                        COALESCE("arieserp_contabilidad".fn_get_TotalImpuestosRetenidos(1), 0))::NUMERIC) ||
    ' PESOS CON ' ||
    LPAD(ROUND(MOD((eccs_comprobante_venta_info.valor_unitario + 
                   COALESCE("arieserp_contabilidad".fn_get_TotalImpuestosTrasladados(1), 0) -
                   COALESCE("arieserp_contabilidad".fn_get_TotalImpuestosRetenidos(1), 0))::numeric, 1) * 100)::varchar, 2, '0') || 
    '/100 M.N.' as total_con_letra

FROM eccs_comprobante_venta
    INNER JOIN eccs_tipo_comprobante ON eccs_comprobante_venta.id_eccs_tipo_comprobante = eccs_tipo_comprobante.id
    INNER JOIN sat_regimenfiscalcfdi as emisorRegimen ON emisorRegimen.id = eccs_tipo_comprobante.id_sat_regimenfiscalcfdi
    INNER JOIN eccs_moneda ON eccs_comprobante_venta.id_eccs_moneda = eccs_moneda.id
    
    -- ============== DOMICILIO SUCURSAL ==============
    INNER JOIN arieserp_sucursal ON arieserp_sucursal.id = eccs_comprobante_venta.id_eccs_sucursal
    INNER JOIN arieserp_empresa ON arieserp_empresa.id = arieserp_sucursal.id_empresa
    INNER JOIN arieserp_sucursal_domicilio ON arieserp_sucursal_domicilio.id_sucursal = arieserp_sucursal.id
        AND arieserp_sucursal_domicilio.id_sucursal = eccs_comprobante_venta.id_eccs_sucursal
        AND arieserp_sucursal_domicilio.predeterminado = true
    
    -- ============== CATALOGOS DE SAT ==============
    INNER JOIN sat_comprobantes ON sat_comprobantes.id = eccs_comprobante_venta.id_sat_comprobantes
    INNER JOIN sat_forma_pago ON sat_forma_pago.id = eccs_comprobante_venta.id_sat_forma_pago
    INNER JOIN sat_exportacion ON sat_exportacion.id = eccs_comprobante_venta.id_sat_exportacion
    INNER JOIN sat_metodo_pago ON sat_metodo_pago.id = eccs_comprobante_venta.id_sat_metodo_pago
    
    -- ============== CLIENTE ==============
    INNER JOIN eccs_cliente ON eccs_comprobante_venta.id_eccs_cliente = eccs_cliente.id
    INNER JOIN eccs_cliente_domicilio ON eccs_cliente_domicilio.id_eccs_cliente = eccs_cliente.id
    INNER JOIN sat_regimenfiscalcfdi ON sat_regimenfiscalcfdi.id = eccs_cliente.id_sat_regimenfiscalcfdi
    INNER JOIN sat_usocfdi ON sat_usocfdi.id = eccs_cliente.id_sat_usocfdi
    
    -- ============== Conceptos ==============
    INNER JOIN eccs_comprobante_venta_info ON eccs_comprobante_venta_info.id_eccs_comprobante_venta = eccs_comprobante_venta.id
    INNER JOIN eccs_producto_servicio ON eccs_producto_servicio.id = eccs_comprobante_venta_info.id_eccs_producto_servicio
    INNER JOIN sat_claveprodserv ON sat_claveprodserv.id = eccs_producto_servicio.id_sat_claveprodserv
    INNER JOIN sat_claveunidad ON sat_claveunidad.id = eccs_producto_servicio.id_sat_claveunidad
    
    -- ============== IMPUESTOS TRASLADOS ==============
    LEFT JOIN eccs_producto_servicio_impuesto psi_traslado ON psi_traslado.id_eccs_producto_servicio = eccs_producto_servicio.id
    LEFT JOIN sat_impuesto sat_impuesto_traslado ON sat_impuesto_traslado.id = psi_traslado.id_sat_impuesto
        AND sat_impuesto_traslado.traslado = true
        AND sat_impuesto_traslado.retencion = false
    LEFT JOIN sat_tipo_factor sat_tipo_factor_traslado ON sat_tipo_factor_traslado.id = sat_impuesto_traslado.id_sat_tipo_factor
    
    -- ============== IMPUESTOS RETENCIONES ==============
    LEFT JOIN eccs_producto_servicio_impuesto psi_retencion ON psi_retencion.id_eccs_producto_servicio = eccs_producto_servicio.id
    LEFT JOIN sat_impuesto sat_impuesto_retencion ON sat_impuesto_retencion.id = psi_retencion.id_sat_impuesto
        AND sat_impuesto_retencion.traslado = false
        AND sat_impuesto_retencion.retencion = true
    LEFT JOIN sat_tipo_factor sat_tipo_factor_retencion ON sat_tipo_factor_retencion.id = sat_impuesto_retencion.id_sat_tipo_factor
WHERE eccs_comprobante_venta.id = 1
GROUP BY 
    eccs_cliente.imagen,
    eccs_tipo_comprobante.serie,
    eccs_comprobante_venta.folio,
    eccs_comprobante_venta.uuid,
    eccs_comprobante_venta.fecha_timbrado,
    eccs_moneda.prefijo,
    eccs_moneda.tc,
    sat_comprobantes.indice,
    sat_comprobantes.descripcion,
    sat_exportacion.codigo,
    sat_exportacion.descripcion,
    sat_metodo_pago.codigo,
    sat_metodo_pago.descripcion,
    sat_forma_pago.codigo,
    sat_forma_pago.descripcion,
    eccs_tipo_comprobante.cp,
    eccs_tipo_comprobante.rfc,
    eccs_tipo_comprobante.numerocertificado,
    eccs_tipo_comprobante.razonsocial,
    emisorRegimen.codigo,
    emisorRegimen.descripcion,
    arieserp_sucursal_domicilio.calle,
    arieserp_sucursal_domicilio.num_ext,
    eccs_cliente.rfc,
    eccs_cliente.nombre,
    eccs_cliente_domicilio.cp,
    eccs_cliente_domicilio.calle,
    sat_regimenfiscalcfdi.codigo,
    sat_regimenfiscalcfdi.descripcion,
    sat_usocfdi.codigo,
    sat_usocfdi.descripcion,
    sat_claveunidad.c_claveunidad,
    sat_claveprodserv.codigo,
    eccs_producto_servicio.descripcion,
    eccs_comprobante_venta_info.cantidad,
    eccs_comprobante_venta_info.valor_unitario,
    eccs_comprobante_venta_info.descuento,
    sat_impuesto_traslado.c_impuesto,
    sat_tipo_factor_traslado.descipcion,
    sat_impuesto_traslado.valor_maximo,
    sat_impuesto_traslado.valor,
    sat_impuesto_retencion.c_impuesto,
    sat_tipo_factor_retencion.descipcion,
    sat_impuesto_retencion.valor_maximo,
    sat_impuesto_retencion.valor;]]>
	</queryString>
